<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.vehicle.business.module.Route" table="bus_conf_route">
        <id name="id" type="java.lang.Integer">
            <generator class="native"/>
        </id>
        <property name="name" type="java.lang.String" column="name"/>
        <property name="detail" type="java.lang.String" column="detail"/>
        <property name="startId" type="java.lang.Integer" column="start_id"/>
        <property name="status" type="java.lang.Integer" column="status"/>
        <property name="endId" type="java.lang.Integer" column="end_id"/>
    </class>
</hibernate-mapping>